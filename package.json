{
  "name": "votreapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "netlify dev",
    "build": "npm-run-all --parallel build:**",
    "serve": "webpack-dev-server --mode development --content-base src --hot --config webpack.dev.js",
    "build:front": "webpack --config webpack.prod.js",
    "build:back": "netlify-lambda build ./src",
    "start": "webpack --open --config webpack.dev.js",
    "postinstall": "netlify-lambda install"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thehunckerma/votreapp.git"
  },
  "author": "thehunckerma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thehunckerma/votreapp/issues"
  },
  "homepage": "https://github.com/thehunckerma/votreapp#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.3.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.2",
    "netlify-lambda": "^2.0.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.0",
    "postcss-loader": "^4.0.2",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "@exodus/schemasafe": "^1.0.0-rc.2",
    "@material/mwc-snackbar": "^0.18.0",
    "axios": "^0.20.0",
    "dotenv": "^8.6.0",
    "faunadb": "^3.0.1",
    "node-json2html": "^1.4.0",
    "node-mailjet": "^3.3.1",
    "smooth-scroll": "^16.1.3"
  }
}
